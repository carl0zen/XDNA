@mixin background-opacity($color, $opacity: 0.3) {
    background: $color; /* The Fallback */
    background: rgba($color, $opacity);
}


  #main-content{
    width: 100%;
    position: relative; 
    @include background-size(cover);
    background:#FCFCFC url(https://raw.github.com/carlosepp/XDNA/master/img/bg.jpg) no-repeat center center fixed;
    .welcome-content{
        margin:0;
        padding: 25px 0;
    }
    #gridBoard{
      background: none;
    }
    .panel{
      background: #C6C7C9;
      border:1px solid transparent;
      @include background-opacity(#C6C7C9, 0.4);
      h3{
        color:$gray;
        font-size: 35px;
        font-weight: 300;

      }
      hr{
        display: none;
      }
      p,a, li{
        color: $black !important;
      }
      span{
        color: #666;
      }
      ul{
        background: none;
        &.bullet li{
          background: url('https://at.avanade.com/organizations/XDNA/Style Library/XD/img/swap-right.png') no-repeat top left;
        }
      }
      &:before{
        display: none;
      }
      &.featured, &[data-panel-id="teamstatus"]{
       
        background: #00ADEE;
        p,a, li, span, h3{
          color:$white !important;
        }
      }//.featured
      .thumbCrop{
        border-radius:100%;
      }
    } //.panel
  }
  #carPaginate {
    a:active {
      box-shadow:none;
    }
    margin-bottom: 25px !important;
    height: 5px !important;
    overflow: hidden;
  }  
  #carNext,
  #carPrev{
    margin-left:0;
    margin-right:0;
    opacity: .9;
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    border: 0 none;
    cursor: pointer;
    font-size: 20px;
    height: auto;
    position: absolute;
    top: 50%;
    width: auto;
    margin-top: -65px;
    opacity: 0.5;
    &:hover, &:focus{
      box-shadow: none;
      opacity: 1;
    }
  }
  &:hover, &:active{
    background-color:#00B0E2;
    opacity: 1;
  }
  .caroufredsel_wrapper{
    margin: 0 auto !important;
  }
  
  img[id*="_PictureUrlImage"]{
    width: 60px; height: 60px;
  }
  .s4-ca > div.s4-notdlg.social-bar,
  .s4-ca > div.s4-notdlg.xd-title {
    display:none
  }
  .s4-tn{
    max-width: 1280px;
    margin:0 auto;
  }

  html input[type="button"], input[type="reset"], input[type="submit"] {
    
    border: none;
    padding: 1.2em 2.2em;
    color: #fff;
    text-highlight: none;
    font-weight: bold;
    font-family: sans-serif;
    font-size: 1.4em;
    height:auto;
  }

  input[type="button"]{
    background-color: $lightBlue;
    &:hover{
      background-color: lighten($lightBlue, 5%)
    }
  }
  input[type="reset"]{
    background-color: $darkBlue;
    &:hover{
      background-color: lighten($darkBlue, 5%);
    }
  }
  input[type="submit"]{
    background-color: $middleBlue;
    &:hover{
      background-color: lighten($middleBlue, 5%);
    }
  }
