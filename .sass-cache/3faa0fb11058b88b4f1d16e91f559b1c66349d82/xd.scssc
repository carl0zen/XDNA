  /* Tablet (landscape) ----------- */
  @media only screen and (max-width: 1024px) {
    body.xd .xd-header{
      padding:20px 0;
      height:100%   
    }
    .xd-menu .s4-tn li.static > .menu-item{
      padding:10px 14px;
    }
  }

  /* Tablet (portrait) ----------- */
  @media only screen and (max-width: 768px) {
    body.xd .xd-header h1 {
      font-size:1.2em;
      position:absolute;
      top:9px;
    }
    body.xd .xd-header .s4-notdlg img{
      margin-top: 13px
    }
    body.xd .xd-header h1{
      top: 21px
    }
    .xd-menu .s4-tn li.static > .menu-item{
      padding:20px 10px;
      -webkit-box-sizing: border-box;
      -moz-box-sizing:border-box;
      -o-box-sizing:border-box;
      -ms-box-sizing:border-box;
      box-sizing:border-box;
    }
    .menu-horizontal{
      width: 100%;
    }
    .menu-horizontal li.static, .menu-horizontal a.static, .menu-horizontal span.static{    
      width:100%;
      float:none;
      display:block;
      margin:0px;
      padding:0px;
    } 
  }
  @media only screen and (min-width: 769px){
    body.xd .menu-icon {
        display: none;
      }
  }
  @media only screen and (max-width: 620px) {
    body.xd .xd-header h1{
      display:none;
    }   
    body.xd #auth{text-align: center; float: none; clear: both; overflow: hidden; height: auto; line-height: normal; margin-top: 1em;}
    body.xd #auth .bold{display: block; text-align: center;}
    body.xd .xd-welcome{float: none;}
    body.xd  #auth .thumb,    body.xd  #auth > div {display: block; height: auto; line-height: normal; margin: 1em auto; overflow: hidden;}
  }
  /* Mobile       ----------- */
  @media only screen and (max-width: 620px) {

    body.xd #carPaginate.loadFade {
      width: auto !important;
    }
    body.xd .static.dynamic-children:active{
       background: #0088af;
    }
    body.xd .xd-welcome #auth{
      margin-top: 20px
    }
    body.xd .xd-welcome #auth .thumb{
      margin:20px auto 0;
    }
    body.xd .menu-horizontal a.dynamic-children span.additional-background:before, 
    body.xd .menu-horizontal span.dynamic-children span.additional-background:before{
      margin-right:-6px
    }
    body.xd .xd-header {
      padding:2em
    }
    body.xd .xd-header .xd-search input{
      height:30px;
      float:none;
      width:240px !important;
      background-color:#fff
    } 
    body.xd .xd-header .columns,  
    body.xd .xd-header .xd-welcome,
    body.xd .xd-header .xd-welcome .welcome,
    body.xd .xd-header .xd-welcome .s4-trc-container-menu,
    body.xd .xd-header .xd-search,
    body.xd .xd-header .xd-search .s4-notdlg,
    body.xd .xd-header .xd-search .ms-sbtable.ms-sbtable-ex.s4-search,
    body.xd .xd-header .xd-search .ms-sbtable.ms-sbtable-ex.s4-search > tbody,
    body.xd .xd-header .xd-search .ms-sbrow,
    body.xd .xd-header .xd-search .ms-sbscopes,
    body.xd .xd-header .xd-search .ms-sbscopes.ms-sbcell {
      display:block;
      width:100%;
      text-align:center;
      padding:0px
    }
    body.xd .xd-header .xd-search{
      margin:2em 0;
      display:none
    } 
    body.xd .xd-header .xd-search .ms-sbcell{
      width:100%;
    }
    body.xd .xd-header .xd-search .ms-sbrow {
      overflow: hidden;
    }
    body.xd .xd-header .xd-search .ms-sbscopes{
      margin-bottom:2em;
      text-align: left;
      height: 30px;
    }   
    body.xd .xd-header .xd-welcome span.welcome{
      font-size:1.5em
    }

    /* MENU       ----------- */
    body.xd xd-menu a, body.xd xd-menu li, body.xd .xd-menu, .menu-horizontal{
      display:block;
      width:100%;
      float:none;
      margin:0px;
      padding:0px
    }     
      
    .menu-item-text{
      padding-left:20px
    }   
    /* Responsive MENU        ----------- */
    body.xd .menu-icon {
      display: block;
    }
    body.xd .menu .ul.dynamic {
      display: block !important;
      left: auto !important;
      top: auto !important;
      position: static !important;
      clear: both !important;
    }
    body.xd ul.root.static {
      overflow: hidden;
      *zoom: 1;
    }
    body.xd #zz15_TopNavigationMenuV4 .menu {
      width: 100%;
    }
    body.xd #zz15_TopNavigationMenuV4 .menu ul {
      width: 100%;
    }
    body.xd #zz15_TopNavigationMenuV4 .menu ul > li {
      width: 100%;
    }
    body.xd #zz15_TopNavigationMenuV4 .menu ul ul > li {
      width: 100%;
    }
    
  }
  
/* ==========================================================================
   Mobile
   ========================================================================== */
  @include mobile-only{
    
    .menu-icon{
      display: block;
    }
    .menu .ul.dynamic{
        display: block !important;
        left: auto !important;
        top: auto !important;
        position: static !important;
        clear: both !important;
        
    }
    ul.root.static{
      @include clearfix;
    }  
    #zz15_TopNavigationMenuV4 .menu{
      
    
      width: 100%;
      ul{
      
        width: 100%;
        > li{
          width: 100%;


        }
        ul > li{
          width: 100%;

        }
      }
    }
  }
  /* ==========================================================================
   Tablet
   ========================================================================== */
  @include tablet-landscape-and-below{
    .s4-ca{
      margin-top: 70px;
    }
  }





 @include tablet-landscape-and-up{
    .menu-icon{
      display: none;
    }

  }

