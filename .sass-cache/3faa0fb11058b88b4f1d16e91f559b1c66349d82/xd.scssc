  .xd-menu {
    background-color: #fff;
    padding: 0 30px;
    border-top: solid 2px $menuBorderColor;
    overflow:visible;
    line-height: 10px;

    .s4-tn li.static > .menu-item {
      height: 100%;
      color: #7FD7F1;
      padding: 17px;
      text-transform: uppercase;
      font-size: 14px;
    }
    .menu ul li:first-child .menu-item {
      color: #999;
    }

    .menu li.selected{
      border-top: 4px solid $menuBorderColor;
      line-height: 2px;
      .menu-item{
        color:#F9F9FB;
      }
    }
    .s4-tn ul.dynamic  {
      border:none; 
    }    
    .s4-tn {
      li.static .menu-item{
        height:100%;
        color:#2F2F2F;
        padding:18px;
        text-transform:uppercase;
        font-size:1.2em;
        &:hover{
          background-color:#f9f9f9;
          text-decoration:none;

        }
      }
      li{
        line-height: 6px;
        font-weight: normal;
      }
    }
    
    .static.dynamic-children li{

      & a:hover{
        background-color: #0088AF
      }
    }
    li.static.dynamic-children.hover{
      background-color: #0088AF;
    }
  }
  .menu-horizontal a.dynamic-children span.additional-background,
  .menu-horizontal span.dynamic-children span.additional-background{
    background-image:none;
    /*background-image:url("img/drop-down-arrow.png");*/
    background-position: right 5px;
    padding-right: 15px;
    &:before{
      content:"‚ñº";
      display:inline-block;   
      float: right;
      font-size: 11px;
      margin-right: -20px;    
    }
  }
  .menu-icon {
    width: 100%;
    right: 0;
    @include transition(all 0.2s ease);
    background-image:url("img/menu.png");
    background-position:70% center;
    background-repeat: no-repeat;
    font-size: 1.5em;
    line-height: 3.3em;
    font-weight: 300;
    background: #fff;
    color: #2e2e2e;
    height:3.3em;
    &:hover {
      background-color: #f9f9f9;
    }
  }
  .mobile-submenu {
